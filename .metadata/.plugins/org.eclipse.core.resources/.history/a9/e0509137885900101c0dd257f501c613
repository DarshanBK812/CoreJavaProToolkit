package library;

public class Book {

	private int boodId;
	private String bookTitle;
	private String bookAuthor;
	private String genre;
	private String publicationYear;

	public Book(int id, String title, String author, String genre, String publish) {
		boodId = id;
		bookTitle = title;
		bookAuthor = author;
		this.genre = genre;
		publicationYear = publish;
	}

}
